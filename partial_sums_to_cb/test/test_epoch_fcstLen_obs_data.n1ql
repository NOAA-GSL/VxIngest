-- should execute less than 0.1 second
SELECT raw METAR.data
FROM
vxdata._default.METAR
USE INDEX (adv_fcstLen_model_fcstValidEpoch_type_docType_version_subset )
WHERE
  METAR.docType  == "obs"
  AND METAR.subset ==  "METAR"
  AND METAR.type ==  "DD"
  AND METAR.version ==  "V01"
  AND METAR.fcstValidEpoch = 1684933200
