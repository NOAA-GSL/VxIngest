<load_spec>
  <email>randy.pierce@noaa.gov</email>
  <initialize_db>true</initialize_db>
  <organization>vxt</organization>
  <group>large_tables</group>
  <connection>
    <management_system>cb</management_system>
    <host>127.0.0.1</host>
    <database>mv_gfs_grid2obs_vsdb1</database>
    <user>met_admin</user>
    <password>met_adm_pwd</password>
  </connection>
  <verbose>false</verbose>
  <insert_size>1</insert_size>
  <stat_header_db_check>true</stat_header_db_check>
  <mode_header_db_check>false</mode_header_db_check>
  <drop_indexes>false</drop_indexes>
  <apply_indexes>true</apply_indexes>
  <load_stat>true</load_stat>
  <load_mode>false</load_mode>
  <load_mpr>false</load_mpr>
  <load_orank>false</load_orank>
  <force_dup_file>true</force_dup_file>
  <folder_tmpl>./data_files/vsdb_data/{stattype}/{cycle}/{model}</folder_tmpl>
  <load_val>
    <field name="stattype">
      <val>anom</val>
      <val>pres</val>
      <val>sfc</val>
      <val>grid2obs</val>
    </field>
    <field name="cycle">
      <val>00Z</val>
      <val>06Z</val>
      <val>12Z</val>
      <val>18Z</val>
    </field>
    <field name="model">
      <val>gfs</val>
      <val>ecm</val>
    </field>
  </load_val>
  <load_xml>true</load_xml>
  <load_note>mv_gfs_grid2obs_vsdb</load_note>
</load_spec>
